{
    "name": "my-props-backend",
    "version": "1.0.0",
    "description": "MyProps Backend API - Basketball Statistics and Analytics",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.js",
        "lint:fix": "eslint src/**/*.js --fix",
        "format": "prettier --write src/**/*.js",
        "setup-views": "node src/scripts/setup-views.js",
        "migrate": "node src/scripts/migrate.js",
        "seed": "node src/scripts/seed.js"
    },
    "keywords": [
        "basketball",
        "statistics",
        "analytics",
        "api",
        "nodejs",
        "express"
    ],
    "author": "Bruno Wendt",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.0.0",
        "compression": "^1.7.4",
        "morgan": "^1.10.0",
        "joi": "^17.9.0",
        "mssql": "^9.1.1",
        "dotenv": "^16.3.1",
        "swagger-jsdoc": "^6.2.5",
        "swagger-ui-express": "^4.6.3"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "jest": "^29.5.0",
        "supertest": "^6.3.3",
        "eslint": "^8.42.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.27.5",
        "prettier": "^2.8.8"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/my-props-backend.git"
    },
    "bugs": {
        "url": "https://github.com/yourusername/my-props-backend/issues"
    },
    "homepage": "https://github.com/yourusername/my-props-backend#readme"
}